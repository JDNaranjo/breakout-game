<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.shape.Circle?>

<BorderPane prefHeight="458.0" prefWidth="572.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.GameController">
   <center>
      <AnchorPane fx:id="gamePane" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: black;" BorderPane.alignment="CENTER">
         <children>
            <ImageView fx:id="ship" fitHeight="50.0" fitWidth="101.0" layoutX="237.0" layoutY="394.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../data/UFO05.png" />
               </image>
            </ImageView>
            <Circle fx:id="ball" fill="WHITE" layoutX="286.0" layoutY="394.0" radius="10.0" stroke="BLACK" strokeType="INSIDE" />
            <Button fx:id="continueButton" layoutX="212.0" layoutY="212.0" mnemonicParsing="false" onAction="#continueNickname" prefHeight="35.0" prefWidth="128.0" text="Continue" visible="false" />
            <Label layoutX="14.0" layoutY="14.0" text="Score: " textFill="WHITE" />
            <Label fx:id="scoreLabel" layoutX="54.0" layoutY="14.0" text="0" textFill="RED" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
